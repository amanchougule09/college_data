   "source": [
    "x=int(input(\"Enter Number\"))\n",
    "\n",
    "def prime(x):\n",
    "    flag=False\n",
    "    if x <2:\n",
    "        print(\"Not Prime\")\n",
    "    for i in range(2,x-1):\n",
    "        if x%i==0:\n",
    "            flag=True\n",
    "    if flag:\n",
    "        print(\"Not Prime\")\n",
    "    else:\n",
    "        print(\"Prime\")\n",
    "\n",
    "prime(x)"
   ]
